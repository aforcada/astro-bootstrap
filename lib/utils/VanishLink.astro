---
const { href, class: className, text, ...props } = Astro.props;
export interface Props {
  href?: string;
  class?: string;
  text?: string;
}
let Tag = '';
if (href) {
  Tag = 'a';
} else {
  Tag = 'span';
}
console.log(`href: ${href} class: ${className}`)
---

<Tag href={href} class={className} {...props}><slot>{text}</slot></Tag>
