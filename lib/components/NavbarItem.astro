---
import Navbar from './Navbar';
const { item } = Astro.props;

import type { Props as NavbarDropdownType } from './NavbarDropdown.astro';
import type {Props as NavbarLinkType } from './NavbarLink.astro'

export interface Props {
  item: NavbarItemType;
}
export interface NavbarItemType extends NavbarDropdownType, NavbarLinkType {}
---

{item.subItems ? <Navbar.Dropdown {...item} /> : <Navbar.Link {...item} />}
