---
import ActiveLink from './ActiveLink.astro';
// className = '' as not using class:list
const { href, disabled, class: className = '' } = Astro.props;
export interface Props {
  href: string;
  disabled?: boolean;
  class?: string;
}
---

<ActiveLink
  {href}
  {disabled}
  disabledClass={' '}
  class={`page-link ${className}`}
>
  <slot />
</ActiveLink>
<!-- class:list={['page-link', className]} -->
