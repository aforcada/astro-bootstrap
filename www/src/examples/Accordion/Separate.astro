---

import {Accordion, AccordionItem, AccordionHeader, AccordionBody } from 'astro-bootstrap';
const id = 'demoAccordion';
---

<Accordion id={id}  >
  <AccordionItem >
    <AccordionHeader id={id} index={0}>
      Header 1 Text
    </AccordionHeader>
    <AccordionBody id={id} index={0}>
      Body 1 Text <strong>header and body can include HTML</strong>
    </AccordionBody>
  </AccordionItem>

</Accordion>
