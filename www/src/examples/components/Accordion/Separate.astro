---

import {Accordion  } from 'astro-bootstrap';
const id = 'demoAccordion';
---

<Accordion id={id}  >
  <Accordion.Item >
    <Accordion.Header id={id} index={0}>
      Header 1 Text
    </Accordion.Header>
    <Accordion.Body id={id} index={0}>
      Body 1 Text <strong>header and body can include HTML</strong>
    </Accordion.Body>
  </Accordion.Item>

</Accordion>
