---
const buttons: Button[] = [{ text: 'Left' }, { text: 'Center' }, { text: 'Right' }];
export interface Button {
  text: string;
}
---

<div class="btn-group" role="group" aria-label="Basic example">
  {
    buttons.map((button) => (
      <button type="button" class="btn btn-primary">
        {button.text}
      </button>
    ))
  }
</div>
