---
import BaseLayout from '../layouts/BaseLayout.astro';
const { title } = Astro.props.frontmatter || Astro.props;
---

<BaseLayout {title}>
  <main>
    <div class="container">
      <div class="row mt-5">
        <div class="col">
          <h1>{title}</h1>
          <slot />
        </div>
      </div>
    </div>
  </main>
</BaseLayout>
